{
  "name": "@err931/textlint-plugin-xliff",
  "version": "0.0.0",
  "description": "XLIFF support for textlint.",
  "keywords": [
    "xliff",
    "plugin",
    "textlint"
  ],
  "homepage": "https://github.com/err931/textlint-plugin-xliff#readme",
  "bugs": {
    "url": "https://github.com/err931/textlint-plugin-xliff/issues"
  },
  "license": "MIT",
  "author": "Minoru Maekawa (https://github.com/err931)",
  "files": [
    "dist"
  ],
  "exports": "./dist/main.js",
  "type": "module",
  "types": "./dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/err931/textlint-plugin-xliff.git"
  },
  "scripts": {
    "build": "npm-run-all -p clean tsc",
    "clean": "rimraf dist",
    "prepare": "simple-git-hooks",
    "test": "env-cmd jest",
    "tsc": "tsc -p ./tsconfig.build.json"
  },
  "dependencies": {
    "@textlint/ast-node-types": "^12.3.0",
    "textlint-filter-rule-node-types": "^1.1.0",
    "unist-util-map": "^3.1.2",
    "vfile-location": "^4.0.1",
    "xast-util-from-xml": "^2.0.1",
    "xast-util-to-xml": "^3.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@textlint/kernel": "^12.3.0",
    "@textlint/types": "^12.3.0",
    "@tsconfig/node-lts-strictest-esm": "^18.12.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/unist": "^2.0.6",
    "env-cmd": "^10.1.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "textlint-rule-no-exclamation-question-mark": "^1.1.0",
    "ts-jest": "^29.0.3",
    "ts-jest-resolver": "^2.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.ts": "deno fmt"
  },
  "simple-git-hooks": {
    "pre-commit": "npx -y lint-staged"
  }
}
